function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(0);
  for (let i = 4; i < width; i += 30) {
    for (let j = 4; j < height; j += 30) {

      stroke(150)
      strokeWeight(5)
      line(i, j, 0, j)
      line(i, 0, i, j)
    }
  }
  
  for (let i = 4; i < width; i += 30) {
    for (let j = 4; j < height; j += 30) {
      
      fill(255)
      noStroke()
      ellipse(i, j, 10, 10)

    }
  }
}
